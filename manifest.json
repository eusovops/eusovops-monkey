{
  "manifest_version": 3,
  "name": "EuSovOps-Monkey",
  "version": "1.2.8",
  "description": "EuSovOps internal userscript manager - run custom JavaScript on any website",
  "author": "Marius Bugaciu @ EUSOVOPS",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    },
    "default_title": "EuSovOps-Monkey"
  },

  "background": {
    "service_worker": "background/service-worker.js"
  },

  "permissions": [
    "storage",
    "scripting",
    "webNavigation",
    "activeTab"
  ],



  "minimum_chrome_version": "88"
}
